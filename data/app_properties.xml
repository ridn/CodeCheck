        <property name="NEW_CREATED_MESSAGE"		value="New Work Created but not yet Saved"></property>
        <property name="WORK_LOADED_MESSAGE"		value="The Work has been Loaded"></property>
        <property name="WORK_SAVED_MESSAGE"		value="The Work has been Saved"></property>
        <property name="SAVE_UNSAVED_WORK_MESSAGE"	value="Do you want to save your unsaved work first?"></property>
    	<property name="SAVE_WORK_TITLE"		value="Save your work"></property>
    	<property name="LOAD_WORK_TITLE"		value="Select file to load"></property>
    	<property name="WORK_FILE_EXT"			value="json"></property>
    	<property name="WORK_FILE_EXT_DESC"		value="Slideshow"></property>

        <!-- WORKSPACE CONTROLS STUFF -->
        <property name="PREV_BUTTON_TEXT"		value="Previous"></property>
        <property name="NEXT_BUTTON_TEXT"		value="Next"></property>
        <property name="HOME_BUTTON_TEXT"		value="Home"></property>
        <property name="RENAME_BUTTON_TEXT"		value="Rename"></property>
        <property name="ABOUT_BUTTON_TEXT"		value="About"></property>
        <property name="STEP_1_BUTTON_1_TEXT"		value="Extract"></property>
        <property name="STEP_2_BUTTON_1_TEXT"		value="Rename"></property>
        <property name="STEP_3_BUTTON_1_TEXT"		value="Unzip"></property>
        <property name="STEP_4_BUTTON_1_TEXT"		value="Extract Code"></property>
        <property name="STEP_5_BUTTON_1_TEXT"		value="Code Check"></property>
        <property name="STEP_5_BUTTON_2_TEXT"		value="View Results"></property>

        <!-- WORKSPACE ERROR MESSAGES -->
        <property name="INVALID_IMAGE_PATH_TITLE"       value="Invalid Image Path"></property>
        <property name="INVALID_IMAGE_PATH_MESSAGE"     value="Image not found"></property>
    </property_list>
    <property_options_list>
    </property_options_list>
</properties>
