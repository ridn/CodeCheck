<?xml version="1.0" encoding="UTF-8"?>
<!--
    This XML file allows us to easily customize the look and language used
    in our program's user interface. Note that this properties file is only
    for the standard controls for an app from the Desktop Java Framework, so
    it would not include things initialized for the workspace, which is
    customizable.
-->
<properties>
    <property_list>
        <property name="APP_TITLE"	    value="Code Check"></property>
    	<property name="APP_LOGO"	    value="CodeCheck.png"></property>
        <property name="UNTITLED_SHOW_TEXT"	    value="Untitled CodeCheck"></property>
    	<property name="APP_PATH_CSS"	    value="style/"></property>
    	<property name="APP_CSS"	    value="cc_style.css"></property>
        <property name="APP_PATH_WORK"      value="work/"></property>

        <!-- PROGRAM ICON FILES -->
        <property name="NEW_ICON"	    value="New.png"></property>
        <property name="LOAD_ICON"	    value="Load.png"></property>
        <property name="PREV_BUTTON_ICON"	    value="prev.png"></property>
        <property name="NEXT_BUTTON_ICON"	    value="next.png"></property>
        <property name="HOME_BUTTON_ICON"	    value="home.png"></property>

        <!-- TOOLTIPS FOR BUTTONS -->
        <property name="NEW_TOOLTIP"		value="Create new Code Check"></property>
        <property name="LOAD_TOOLTIP"		value="Load an existing Code Check"></property>
        <property name="PREV_BUTTON_TOOLTIP"		value="Go to previous step"></property>
        <property name="NEXT_BUTTON_TOOLTIP"		value="Go to next step"></property>
        <property name="HOME_BUTTON_TOOLTIP"		value="Go to first step"></property>

        <!-- LABELS -->
        <property name="STEP_1_TITLE"		value="Step 1: Extract Submissions"></property>
        <property name="STEP_2_TITLE"		value="Step 2: Rename Student Submissions"></property>
        <property name="STEP_3_TITLE"		value="Step 3: Unzip Student Submissions"></property>
        <property name="STEP_4_TITLE"		value="Step 4: Extract Source Code"></property>
        <property name="STEP_5_TITLE"		value="Step 5: Code Check"></property>
        <property name="STEP_1_HINT"		value="Select the submissions below from which to extract student work"></property>
        <property name="STEP_2_HINT"		value="Click the Rename button to rename all submissions"></property>
        <property name="STEP_3_HINT"		value="Select student submissions and click Unzip"></property>
        <property name="STEP_4_HINT"		value="Select students and click Extract Code"></property>
        <property name="STEP_5_HINT"		value="Select students and click Code Check"></property>
        <property name="STEP_1_PROGRESS"		value="Extraction Progress"></property>
        <property name="STEP_2_PROGRESS"		value="Rename Progress"></property>
        <property name="STEP_3_PROGRESS"		value="Unzip Progress"></property>
        <property name="STEP_4_PROGRESS"		value="Code Progress"></property>
        <property name="STEP_5_PROGRESS"		value="Check Progress"></property>
        <property name="FILE_TYPE_LABEL"		value="Source File Types"></property>
        <property name="FILE_TYPE_1"		value=".java"></property>
        <property name="FILE_TYPE_2"		value=".c, .h, .cpp"></property>
        <property name="FILE_TYPE_3"		value=".js"></property>
        <property name="FILE_TYPE_4"		value=".cs"></property>
        <property name="FILE_TYPE_CUSTOM"		value="Enter source type(s)"></property>

        <!-- ERROR MESSAGES -->
        <property name="CLOSE_BUTTON_TEXT"              value="Close"></property>

        <property name="NEW_ERROR_MESSAGE"              value="An error occured creating New work."></property>
        <property name="NEW_ERROR_TITLE"		value="New Work Error"></property>
        <property name="LOAD_ERROR_MESSAGE"             value="An error occured loading work."></property>
        <property name="LOAD_ERROR_TITLE"		value="Load Work Error"></property>
        <property name="SAVE_ERROR_MESSAGE"             value="An error occured saving work."></property>
        <property name="SAVE_ERROR_TITLE"		value="Save Work Error"></property>
        <property name="PROPERTIES_LOAD_ERROR_MESSAGE"	value="An error occured loading a properties file."></property>
        <property name="PROPERTIES_LOAD_ERROR_TITLE"	value="Properties File Loading Error"></property>
        <property name="DUPLICATE_IMAGE_TITLE"	value="Duplicate Image"></property>
        <property name="DUPLICATE_IMAGE_MESSAGE"	value="Please select another image."></property>
        <property name="DUPLICATE_FOUND_TITLE"	value="Duplicates Found"></property>
        <property name="DUPLICATE_FOUND_MESSAGE"	value="Some images were not added."></property>

        <!-- VERIFICATION MESSAGES -->

        <property name="NEW_COMPLETED_MESSAGE"		value="New Work Ready for Editing"></property>
        <property name="NEW_COMPLETED_TITLE"		value="New Work Created"></property>
        <property name="LOAD_COMPLETED_MESSAGE"		value="File Loading Completed and ready for editing"></property>
        <property name="LOAD_COMPLETED_TITLE"		value="Loading Completed"></property>
